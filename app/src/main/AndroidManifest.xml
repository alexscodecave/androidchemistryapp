<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alexb.chemistryapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chemistrycontentartboardhdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chemistrycontentartboardhdpi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PeriodicTable"
            android:screenOrientation="landscape" />
        <activity android:name=".famouschemists"></activity>
    </application>

</manifest>